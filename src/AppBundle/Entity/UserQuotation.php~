<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="user_quotation")
 *
 */
class UserQuotation
{

	/**
	 * @ORM\Id
	 * @ORM\Column(type="integer")
	 * @ORM\GeneratedValue(strategy="AUTO")
	 *
	 * @var \Int
	 */
	protected $id;

	/**
	 * @ORM\Column(type="datetime")
	 *
	 * @var \DateTime
	 */
	protected $date = null;

	/**
	 * @ORM\Column(type="text")
	 *
	 * @var string
	 */
	protected $text;

	/**
	 * @ORM\Column(type="text")
	 *
	 * @var string
	 */
	protected $userInfos;
}
